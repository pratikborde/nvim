{
  "suggest.triggerAfterInsertEnter": true,
  "prettier.requireConfig": true,
  "tslint.enable": true,
  "tslint.autoFixOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "markdown",
    "Markdown",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "coc.preferences.hoverTarget": "float",
  "suggest.floatEnable": true,
  "importCost.typescriptExtensions": [
    "\\.tsx?$"
  ],
  "importCost.javascriptExtensions": [
    "\\.jsx?$",
    "\\.js?$"
  ],
  "importCost.bundleSizeDecoration": "both",
  "importCost.showCalculatingDecoration": true,
  "importCost.debug": false,
  "todolist.monitor": true,
  "diagnostic.errorSign": "✘",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic-languageserver.filetypes": {
    "sh": "shellcheck"
  },
  "eslint.enable": false,
  "eslint.filetypes": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "typescript.referencesCodeLens.enable": true,
  "git.enableGutters": false,
  "git.realtimeGutters": false,
  // "git.virtualTextPrefix": "     ~  ",
  "git.addGBlameToVirtualText": true,
  "codeLens.enable": false,
  "tsserver.enableJavascript": true,
  "snippets.ultisnips.directories": [
    "~/.vim/plugged/vim-snippets/snippets",
    "~/.vim/plugged/vim-javascript-snippets/snippets/javascript"
  ],
  "snippets.extends": {
    "javascriptreact": ["javascript"],
    "typescriptreact": ["javascript", "javascript-jasmine-arrow"],
    "typescript": ["javascript", "javascript-jasmine-arrow"]
  },
  "prettier.eslintIntegration": false,
  "suggest.noselect": false,
  "suggest.enablePreview": true,
  "suggest.removeDuplicateItems": true,
  "coc.preferences.jumpCommand": "drop",
  "coc.preferences.rootPatterns": [".git", ".vim", ".vscode"],
  "diagnostic.checkCurrentLine": true,
  "diagnostic.virtualText": true,
  "tsserver.npm": "/usr/local/bin/npm"
}
